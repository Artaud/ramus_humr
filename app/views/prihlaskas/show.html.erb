<%- model_class = Prihlaska -%>
<div class="page-header">
  <h1><%=t '.title', :default => Prihlaska %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:jmeno_prijmeni) %></dt>
      <dd><pre class="prettyprint"><%= @prihlaska.jmeno_prijmeni %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:kontakt) %></dt>
      <dd><pre class="prettyprint"><%= @prihlaska.kontakt %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:vzkaz) %></dt>
      <dd><pre class="prettyprint"><%= @prihlaska.vzkaz %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              prihlaskas_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_prihlaska_path(@prihlaska), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              prihlaska_path(@prihlaska),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

